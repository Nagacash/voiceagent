# .env.example - Users copy this to .env and fill in their credentials
# Voice Agent Configuration

# OpenClaw Setup
VITE_OPENCLAW_ENDPOINT=http://127.0.0.1:18789
VITE_OPENCLAW_TOKEN=your_openclaw_token_here

# OpenAI Configuration
VITE_OPENAI_API_KEY=sk-your-openai-key-here
VITE_OPENAI_MODEL=gpt-3.5-turbo

# Telegram Bot
VITE_TELEGRAM_BOT_TOKEN=123456:ABC-DEF1234ghIkl-zyx57W2v1u123ew11
VITE_TELEGRAM_CHAT_ID=@your_username

# Discord Bot
VITE_DISCORD_BOT_TOKEN=your_discord_bot_token_here
VITE_DISCORD_CHANNEL_ID=your_channel_id_here

# Claude (Anthropic)
VITE_CLAUDE_API_KEY=sk-ant-api03-your-claude-key-here

# Custom AI Endpoint
VITE_CUSTOM_AI_ENDPOINT=https://your-ai-endpoint.com/api
VITE_CUSTOM_AI_KEY=your-custom-api-key